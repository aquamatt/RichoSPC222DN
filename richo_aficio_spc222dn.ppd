    userdict /HQCopies currentpagedevice /NumCopies get put
}{
    userdict /#copies get /HQCopies exch def 
} ifelse

<< /OutputDevice (ebdmulti;bits=1) >> setpagedevice 

/languagelevel where {pop languagelevel}{1} ifelse
2 ge 
{ 
    1 dict dup /NumCopies userdict /HQCopies get put setpagedevice
}{
    userdict /#copies HQCopies put 
} ifelse 
"
*End
*HQPrintMode 662/High Quality: "/languagelevel where {pop languagelevel}{1} ifelse
2 ge 
{ 
    userdict /HQCopies currentpagedevice /NumCopies get put
}{
    userdict /#copies get /HQCopies exch def 
} ifelse

<< /OutputDevice (ebdmulti;bits=2) >> setpagedevice 

/languagelevel where {pop languagelevel}{1} ifelse
2 ge 
{ 
    1 dict dup /NumCopies userdict /HQCopies get put setpagedevice
}{
    userdict /#copies HQCopies put 
} ifelse
"
*HQPrintMode 664/Best Quality: "/languagelevel where {pop languagelevel}{1} ifelse
2 ge 
{ 
    userdict /HQCopies currentpagedevice /NumCopies get put
}{
    userdict /#copies get /HQCopies exch def 
} ifelse

<< /OutputDevice (ebdmulti;bits=4) >> setpagedevice 

/languagelevel where {pop languagelevel}{1} ifelse
2 ge 
{ 
    1 dict dup /NumCopies userdict /HQCopies get put setpagedevice
}{
    userdict /#copies HQCopies put 
} ifelse
"
*End
